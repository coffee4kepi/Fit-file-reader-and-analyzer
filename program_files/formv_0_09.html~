	<head>
	</head>

	<body>
		
		<h2 align="center"> 
		File FIT da decodificare:
		<form name="azioni" method="post" action='' target="dati">
			<input type="file" name="filename">
			<input type="button" name="vedi" value="visualizza" onclick="submit_visualizza()">
			<input type="button" name="analizza" value="analizza" onclick="submit_analizza()">
		</form>
		</h2>
	<script>
		function submit_visualizza(){
			document.azioni.action="pagina_list.php";
			document.azioni.submit();
		
		}
		function submit_analizza(){
			document.azioni.action="fit_file_pagina_class_analize.php";
			document.azioni.submit();
		}
	</script>

	</body>



